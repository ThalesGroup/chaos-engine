                experimentState.FINALIZING as FINALIZING,
                experimentState.FINISHED as FINISHED,
                experimentState.SELF_HEALING as SELF_HEALING,
                experimentState.STARTED as STARTED,
                experimentState.STARTING as STARTING

                | table CREATED, FAILED,FINALIZING,FINISHED, SELF_HEALING, STARTED,STARTING
                |transpose
          </query>
            <earliest>rt-1m</earliest>
            <latest>rtnow</latest>
