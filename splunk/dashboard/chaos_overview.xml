  <row>
    <panel>
      <title>Overall Recovery Duration</title>
      <table>
        <search>
          <query>sourcetype="container:log" finalExperimentDuration | spath input=log |table platform
                        experimentMethod finalExperimentDuration.seconds experimentid
                        |stats count(experimentid) as "Experiment Count"
                        ,avg(finalExperimentDuration.seconds) as "AVG Recovery Duration"
                        ,median(finalExperimentDuration.seconds) as "Median Duration"
                        ,stdev(finalExperimentDuration.seconds) as "StdDev Duration"
                        ,max(finalExperimentDuration.seconds) as "Max Recovery Duration"
                        ,min(finalExperimentDuration.seconds) as " Min Recovery Duration"
                        by platform,experimentMethod
                    </query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">25</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="platform">
          <colorPalette type="map">{"CloudFoundryApplicationPlatform":#708794,"CloudFoundryContainerPlatform":#4FA484,"AwsEC2Platform":#3FE4ED,"AwsRDSPlatform":#ED3FCA,"KubernetesPlatform":#3E89ED}</colorPalette>
        </format>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Experiment Recovery Duration Per Container And Method</title>
      <table>
        <search>
          <query>sourcetype="container:log" ChaosExperimentEvent | spath input=log | eval startTime=_time
                | search ChaosExperimentEvent.message="Starting new experiment"


                |join ChaosExperimentEvent.experimentId [search sourcetype="container:log" ChaosExperimentEvent
                        | spath input=log
                | search ChaosExperimentEvent.message="Experiment finished.*"
                | eval finishTime=_time]
                | eval experimentDuration=finishTime-startTime
                        | eval experimentDuration=experimentDuration-30
              |stats count(ChaosExperimentEvent.experimentId) as "Experiment Count"
                        ,avg(experimentDuration) as "AVG Recovery Duration"
                        ,median(experimentDuration) as "Median Duration"
                        ,stdev(experimentDuration) as "StdDev Duration"
                        ,max(experimentDuration) as "Max Recovery Duration"
                        ,min(experimentDuration) as " Min Recovery Duration"
                        by
              ChaosExperimentEvent.targetContainer.containerType,ChaosExperimentEvent.experimentType,ChaosExperimentEvent.experimentMethod,
              ChaosExperimentEvent.targetContainer.aggregationIdentifier
              | rename ChaosExperimentEvent.targetContainer.containerType as "Container Type"
              | rename ChaosExperimentEvent.experimentType as "Experiment Type"
              | rename ChaosExperimentEvent.experimentMethod as "Experiment Method"
              | rename ChaosExperimentEvent.targetContainer.aggregationIdentifier as "Container Name (Aggregation Identifier)"
                | sort - "Median Duration"
            </query>
          <earliest>$field1.earliest$</earliest>
          <latest>$field1.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="Container Type">
